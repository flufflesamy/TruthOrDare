<Window x:Class="TruthOrDare.ComboDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TruthOrDare"
        mc:Ignorable="d"
        Title="ComboDialog" Height="120" Width="329">
    <StackPanel>
        <TextBlock Text="Enter Name" FontSize="16" />
        <ComboBox x:Name="ResponseComboBox" FontSize="16" />
        <Button Content="OK" Click="OKButton_Click" FontSize="16" IsDefault="True"/>
    </StackPanel>
</Window>
